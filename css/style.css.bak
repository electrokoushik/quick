/*  Common CSS start  */
body{}
/*  Common CSS end  */

/*  Header start  */
header{
	padding: 5px 0px;
	border-bottom: 3px solid #fafbfb;
}
.container {
	margin: 0 auto;
	width: 90%;
	overflow: hidden;
}
.logo {
	width: 10%;
	float: left;
}
.logo a{
	display: block;
}
.logo a img{
	width: 100%;
}
.menu {
	width: 50%;
	float: right;
}
.menu ul{
	display: block;
	float: right;
}
.menu ul li{
	list-style: none;
	float: left;
}
.menu ul li a{
	display: inline-block;
	text-decoration: none;
	padding: 1px 25px;
	font-size: 16px;
	font-family: sans-serif;
	font-weight: 400;
	color: #0F2656;
}
.menu ul li:last-child a{
	padding-right: 0;
}
.menu ul li a:hover{
	color: #008AFF;
}
/* Header end */

/* Slogan Start */
.slogan_sec{
    width: 60%;
    margin: 0 auto;
    text-align: center;
}

.slogan_text h1 {
    font-size: 45px;
    color: #0f2656;
    line-height: 50px;
    margin: 70px 0px 30px;
}

.slogan_text h1 span{
    color: #008aff;
}

.slogan_text p{
    margin: 0;
}

.slogan_img img{
    width: 75%;
    margin: 40px 0 0;
}
/* Slogan End */

/*Slider Image Start*/
.project_sec{
    width: 60%;
    margin: 25px auto 70px;
}

.project_text{
    display: inline-block;
    width: 48%;
    text-align: right;
}

.project_text h1{
    margin: 0;
    font-size: 40px;
    color: #f15969;
}

.project_text p{
    color: #464646;
    font-size: 20px;
}

.project_ideas{
    width: 48%;
    float: right;
}

.project_ideas h1{
    font-size: 40px;
    margin: 0;
    color: #0f2656;
}
/*Project End */

/*  Feature Section start  */
.feature{
	padding: 30px 0 50px;
}

.f_content{
	overflow: hidden;
}
.f_left{
	padding: 0 0 0 30px;
	width: 45%;
	text-align: right;
	float: left;
}
.f_left h2{
	margin: 0;
	font-size: 40px;
	color: #F15767;
}
.f_left p{
	margin: 10px 0 0;
	padding: 0 10px;
	font-weight: 300;
	line-height: 25px;
}
.f_right{
	padding: 0 30px 0 0;
	width: 45%;
	float: right;
}
.f_right h1{
	margin: 0;
    font-size: 46px;
}
/*  Feature Section end  */


/*  Service CSS start  */
.service{
	padding: 40px 0 70px;
}
.service-content{
	overflow: hidden;
}
.service-box{
	margin-right: 2%;
	float: left;
	width: 28%;
	padding: 50px 2%;
}
.service-box:last-child{
	margin-right: 0;
}
.pink{
	background-color: #FDEBED;
}
.blue{
	background-color: #EBF9F4;
}
.pinklight{
	background-color: #FFF7E8;
}
.s-img{}
.s-img img{
	width: 100%;
}
.s_h2{
	margin: 20px 0;
	font-size: 25px;
}
.s_p{
	margin: 0;
	line-height: 25px;
}
/*  Service CSS end  */

/*  Footer CSS start  */
footer{
	padding: 15px 0;
	background-color: #008aff;
}
.footer-content{
	text-align: center;
}
.footer-content p{
	color: #9EC7FF;
}
.footer-content p a{
	text-decoration: none;
	text-transform: uppercase;
	letter-spacing: 3px;
	color: #ffffff;
}
.footer-content p a:hover{
	color: #0F2656;
}
/*  Footer CSS end  */